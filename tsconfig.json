{
    "compilerOptions": {
        "module": "umd",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "downlevelIteration": true,
        "lib": ["ES2021", "DOM", "DOM.Iterable"],
        "declaration": true,
        "skipLibCheck": true,
        "outDir": "dist/"
    },
    "include": ["source/**/*"],
    "typedocOptions": {
        "name": "KoAJAX",
        "excludeExternals": true,
        "excludePrivate": true,
        "readme": "./ReadMe.md",
        "out": "docs/"
    }
}
